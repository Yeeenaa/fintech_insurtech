        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"></script>
        <!-- Contact form JS-->
        <script src="assets/mail/jqBootstrapValidation.js"></script>
        <script src="assets/mail/contact_me.js"></script>
        <!-- Core theme JS-->
        <script src="js/scripts.js"></script>
        <script>
            $("#next").click(function(){
                var radioVal = $('input[name="package"]:checked').val();
                if(radioVal == 'basic'){
                    $(location).attr('href', 'http://localhost:3000/result');
                }
                else if(radioVal=='gold'){
                    $(location).attr('href', 'http://localhost:3000/result2');
                }
                else{
                    $(location).attr('href', 'http://localhost:3000/result3');
                }
                console.log(radioVal)      
            })
        </script>
          
    </body>
</html>